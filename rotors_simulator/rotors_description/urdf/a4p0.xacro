<robot name="a4p0" xmlns:xacro="http://ros.org/wiki/xacro">

  <!-- Global properties -->
  <xacro:property name="namespace" value="$(arg namespace)" />
  <xacro:property name="rotor_velocity_slowdown_sim" value="10" />
  <xacro:property name="use_mesh" value="true" />
  <xacro:property name="yaml_path" value="$(arg yaml_path)"/>
  <xacro:property name="serial_device" value="$(arg serial_device)" />
  <xacro:property name="enable_wind" value="$(arg enable_wind)" />

  <!-- Constants -->
  <xacro:property name="PI" value="3.1415926535897931" />

  <!-- Servo PD controller paramters, tune after inertias of actuated links are set -->
  <xacro:property name="ctrl_surf_srv_params">
    <slew>3.3</slew>
    <p>0.0004</p>
    <d>0.00002</d>
  </xacro:property>
  <xacro:property name="tail_srv_params">
    <slew>3.3</slew>
    <p>0.05</p>
    <d>0.005</d>
  </xacro:property>
  <xacro:property name="wing_srv_params">
    <slew>0.52</slew>
    <p>50000</p>
    <d>0</d>
  </xacro:property>

  <!-- Propulsion dynamics -->
  <xacro:property name="tau_up_prop" value="0.2" />   <!--0.3-->
  <xacro:property name="tau_down_prop" value="0.4" /> <!--0.6-->
  <xacro:property name="tau_up_fan" value="0.1" />   <!--0.15-->
  <xacro:property name="tau_down_fan" value="0.2 "/>  <!--0.3-->

  <!-- Control surface effectiveness -->
  <xacro:property name="ail_dLdu" value="1" />
  <xacro:property name="ele_dLdu" value="4" />
  <xacro:property name="rud_dLdu" value="1.5" />

  <!-- Flexing gear properties -->
  <xacro:property name="k_gear" value="2000" />
  <xacro:property name="d_gear" value="200" />
  <xacro:property name="gear_flex_on" value="false" />

  <!-- Included URDF files -->
  <xacro:include filename="$(find rotors_description)/urdf/fixed_wing_X_base.xacro" />

  <!-- Fuselage and landing gear -->
  <link name="${namespace}/base_link">
      <inertial>
        <mass value="39.5" /> <!--52.1-->
        <xacro:box_inertia x="2" y="0.4" z="0.4" mass="39.5" />
        <origin xyz="-0.07 0 -0.1" rpy="0 0 0" />
        <!--<origin xyz="-0.02 0 -0.1" rpy="0 0 0" />-->
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://rotors_description/meshes/a4p0/fuselage.obj" scale="1 1 1" />
        </geometry>
      </visual>
      <!--<visual>
        <origin xyz="0.4 -0.56 -0.6" rpy="1.57 0 0" />
        <geometry>
          <cylinder radius ="0.07" length="0.05"/>
        </geometry>
      </visual>
      <collision>
        <origin xyz="0.4 -0.56 -0.6" rpy="0 0 0" />
        <geometry>
          <sphere radius="0.07"/>
        </geometry>
      </collision>
      <visual>
        <origin xyz="0.4 0.56 -0.6" rpy="1.57 0 0" />
        <geometry>
          <cylinder radius ="0.07" length="0.05"/>
        </geometry>
      </visual>
      <collision>
        <origin xyz="0.4 0.56 -0.6" rpy="0 0 0" />
        <geometry>
          <sphere radius="0.07"/>
        </geometry>
      </collision>-->
      <visual>
        <origin xyz="-2.1 0 -0.55" rpy="1.57 0 0" />
        <geometry>
          <cylinder radius ="0.07" length="0.05"/>
        </geometry>
      </visual>
      <collision>
        <origin xyz="-2.1 0 -0.55" rpy="0 0 0" />
        <geometry>
          <sphere radius="0.07"/>
        </geometry>
      </collision>
      <collision>
        <origin xyz="0 0 -0.1" rpy="0 0 0" />
        <geometry>
          <box size="2 0.5 0.5" />
        </geometry>
      </collision>
  </link>

  <xacro:if value="${gear_flex_on}">
    <joint name="${namespace}/main_gear_left_joint" type="prismatic">
       <origin xyz="0.4 0.56 -0.6" rpy="0 0 0" />
       <axis xyz="0 0 1" />
       <limit upper="0.5" lower="-0.5" effort="5000" velocity="5"/>
       <parent link="${namespace}/base_link" />
       <child link="${namespace}/main_gear_left" />
       <dynamics damping="${d_gear}"/>
    </joint>
    <gazebo reference="${namespace}/main_gear_left_joint">
      <implicitSpringDamper>true</implicitSpringDamper>
      <springStiffness>${k_gear}</springStiffness>
    </gazebo>
  </xacro:if>
  <xacro:unless value="${gear_flex_on}">
    <joint name="${namespace}/main_gear_left_joint" type="fixed">
       <origin xyz="0.4 0.56 -0.6" rpy="0 0 0" />
       <parent link="${namespace}/base_link" />
       <child link="${namespace}/main_gear_left" />
    </joint>
  </xacro:unless>
  <link name="${namespace}/main_gear_left">
      <inertial>
        <mass value="0.5" />
        <xacro:box_inertia x="0.1" y="0.1" z="0.1" mass="0.5" />
        <origin xyz="-0.2 0 -0.1" rpy="0 0 0" />
      </inertial>
      <collision>
        <geometry>
          <sphere radius="0.07"/>
        </geometry>
      </collision>
  </link>

  <xacro:if value="${gear_flex_on}">
    <joint name="${namespace}/main_gear_right_joint" type="prismatic">
       <origin xyz="0.4 -0.56 -0.6" rpy="0 0 0" />
       <axis xyz="0 0 1" />
       <limit upper="0.5" lower="-0.5" effort="5000" velocity="5"/>
       <parent link="${namespace}/base_link" />
       <child link="${namespace}/main_gear_right" />
       <dynamics damping="${d_gear}"/>
    </joint>
    <gazebo reference="${namespace}/main_gear_right_joint">
      <implicitSpringDamper>true</implicitSpringDamper>
      <springStiffness>${k_gear}</springStiffness>
    </gazebo>
  </xacro:if>
  <xacro:unless value="${gear_flex_on}">
    <joint name="${namespace}/main_gear_right_joint" type="fixed">
       <origin xyz="0.4 -0.56 -0.6" rpy="0 0 0" />
       <parent link="${namespace}/base_link" />
       <child link="${namespace}/main_gear_right" />
    </joint>
  </xacro:unless>
  <link name="${namespace}/main_gear_right">
      <inertial>
        <mass value="0.5" />
        <xacro:box_inertia x="0.1" y="0.1" z="0.1" mass="0.5" />
        <origin xyz="-0.2 0 -0.1" rpy="0 0 0" />
      </inertial>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <sphere radius="0.07"/>
        </geometry>
      </collision>
  </link>

  <joint name="${namespace}/main_gear_left_vis_joint" type="fixed">
     <origin xyz="0.4 0.56 -0.6" rpy="0 0 0" />
     <parent link="${namespace}/base_link" />
     <child link="${namespace}/main_gear_left_vis" />
  </joint>
  <link name="${namespace}/main_gear_left_vis">
     <inertial>
        <mass value="0.001" />
        <xacro:box_inertia x="0.001" y="0.001" z="0.001" mass="0.001" />
     </inertial>
     <visual>
       <origin xyz="0 0 0" rpy="1.57 0 0" />
       <geometry>
         <cylinder radius ="0.07" length="0.04"/>
       </geometry>
     </visual>
  </link>
  <gazebo reference="${namespace}/main_gear_left_vis">
     <material>Gazebo/Grey</material>
  </gazebo>

  <joint name="${namespace}/main_gear_right_vis_joint" type="fixed">
     <origin xyz="0.4 -0.56 -0.6" rpy="0 0 0" />
     <parent link="${namespace}/base_link" />
     <child link="${namespace}/main_gear_right_vis" />
  </joint>
  <link name="${namespace}/main_gear_right_vis">
     <inertial>
        <mass value="0.001" />
        <xacro:box_inertia x="0.001" y="0.001" z="0.001" mass="0.001" />
     </inertial>
     <visual>
       <origin xyz="0 0 0" rpy="1.57 0 0" />
       <geometry>
         <cylinder radius ="0.07" length="0.04"/>
       </geometry>
     </visual>
  </link>
  <gazebo reference="${namespace}/main_gear_right_vis">
     <material>Gazebo/Grey</material>
  </gazebo>

  <joint name="${namespace}/pitot" type="fixed">
     <parent link="${namespace}/base_link" />
     <child link="${namespace}/pitot" />
  </joint>
  <link name="${namespace}/pitot">
     <inertial>
        <mass value="0.001" />
        <xacro:box_inertia x="0.001" y="0.001" z="0.001" mass="0.001" />
     </inertial>
     <visual>
       <origin xyz="1.6 0 -0.3" rpy="0 1.57 0" />
       <geometry>
         <cylinder radius ="0.01" length="1.0"/>
       </geometry>
     </visual>
     <visual>
       <origin xyz="2.2 0 -0.3" rpy="0 1.57 0" />
       <geometry>
         <cylinder radius ="0.005" length="0.2"/>
       </geometry>
     </visual>
     <visual>
       <origin xyz="1.9 0 -0.35" rpy="0 0 0" />
       <geometry>
         <cylinder radius ="0.003" length="0.1"/>
       </geometry>
     </visual>
     <visual>
       <origin xyz="1.9 0 -0.3" rpy="1.57 0 0" />
       <geometry>
         <cylinder radius ="0.003" length="0.2"/>
       </geometry>
     </visual>
  </link>
  <gazebo reference="${namespace}/pitot">
     <material>Gazebo/Grey</material>
  </gazebo>

  <!-- Wing -->
  <joint name="${namespace}/wing_joint" type="revolute">
    <origin xyz="0.05 0 0.24" rpy="0 0 0" />
    <axis xyz="0 1 0" />
    <parent link="${namespace}/base_link" />
    <child link="${namespace}/wing" />
    <limit upper="0" lower="-1.57" effort="10" velocity="20"/>
    <dynamics damping="25" friction="0" />
  </joint>
  <link name="${namespace}/wing">
      <inertial>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <mass value="33" />
        <xacro:box_inertia x="0.65" y="3.5" z="0.12" mass="33" />
      </inertial>
      <visual>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh filename="package://rotors_description/meshes/a4p0/wing.obj" scale="1 1 1" />
        </geometry>
      </visual>
      <collision>
        <origin xyz="-0.1 0 0" rpy="0 0 0" />
        <geometry>
          <box size="0.65 3.5 0.12" />
        </geometry>
      </collision>
  </link>

  <joint name="${namespace}/aileron_left_joint" type="revolute">
    <origin xyz="-0.307 1.1 -0.006" rpy="0 0 0" />
    <axis xyz="0 -1 0" />
    <parent link="${namespace}/wing" />
    <child link="${namespace}/aileron_left" />
    <limit upper="0.53" lower="-0.53" effort="10" velocity="20"/>
  </joint>
  <link name="${namespace}/aileron_left">
    <inertial>
      <mass value="0.001" />
      <xacro:box_inertia x="0.1" y="0.1" z="0.1" mass="0.001" />
    </inertial>
    <visual>
      <origin xyz="0.307 -1.1 0.006" rpy="0 0 0" />
      <geometry>
          <mesh filename="package://rotors_description/meshes/a4p0/aileron-left.obj" scale="1 1 1" />
      </geometry>
    </visual>
  </link>

  <joint name="${namespace}/aileron_right_joint" type="revolute">
    <origin xyz="-0.307 -1.1 -0.006" rpy="0 0 0" />
    <axis xyz="0 -1 0" />
    <parent link="${namespace}/wing" />
    <child link="${namespace}/aileron_right" />
    <limit upper="0.53" lower="-0.53" effort="10" velocity="20"/>
  </joint>
  <link name="${namespace}/aileron_right">
    <inertial>
      <mass value="0.001" />
      <xacro:box_inertia x="0.1" y="0.1" z="0.1" mass="0.001" />
    </inertial>
    <visual>
      <origin xyz="0.307 1.1 0.006" rpy="0 0 0" />
      <geometry>
          <mesh filename="package://rotors_description/meshes/a4p0/aileron-right.obj" scale="1 1 1" />
      </geometry>
    </visual>
  </link>

  <!-- Tail -->
  <joint name="${namespace}/stabilator_joint" type="revolute">
    <origin xyz="-1.96 0 -0.06" rpy="0 0 0" />
    <axis xyz="0 -1 0" />
    <parent link="${namespace}/base_link" />
    <child link="${namespace}/stabilator" />
    <limit upper="0.53" lower="-0.53" effort="10" velocity="20"/>
  </joint>
  <link name="${namespace}/stabilator">
    <inertial>
      <mass value="0.001" />
      <xacro:box_inertia x="0.1" y="0.1" z="0.1" mass="0.001" />
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
          <mesh filename="package://rotors_description/meshes/a4p0/stabilator.obj" scale="1 1 1" />
      </geometry>
    </visual>
  </link>

  <joint name="${namespace}/rudder_joint" type="revolute">
    <origin xyz="-2.110 0 -0.06" rpy="0 0 0" />
    <axis xyz="0 0 1" />
    <parent link="${namespace}/base_link" />
    <child link="${namespace}/rudder" />
    <limit upper="0.53" lower="-0.53" effort="10" velocity="20"/>
  </joint>
  <link name="${namespace}/rudder">
    <inertial>
      <mass value="0.001" />
      <xacro:box_inertia x="0.1" y="0.1" z="0.1" mass="0.001" />
    </inertial>
    <visual>
      <origin xyz="0.15 0 0" rpy="0 0 0" />
      <geometry>
          <mesh filename="package://rotors_description/meshes/a4p0/rudder.obj" scale="1 1 1" />
      </geometry>
    </visual>
  </link>

  <!-- Main propellers -->
  <joint name="${namespace}/propeller_P0_joint" type="continuous">
    <origin xyz="0.32 1.415 0" rpy="0 ${0.5*PI} 0" />
    <axis xyz="0 0 1" />
    <parent link="${namespace}/wing" />
    <child link="${namespace}/propeller_P0" />
  </joint>
  <link name="${namespace}/propeller_P0">
    <inertial>
      <mass value="0.0125" />
      <xacro:cylinder_inertia r="0.14" h="0.02" mass="0.0125" />
    </inertial>
    <visual>
      <origin xyz="0.0035 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://rotors_description/meshes/tiltwing/iris_prop_ccw.dae" scale="2.7 2.7 2.7"/>
      </geometry>
    </visual>
    <visual>
      <origin xyz="0 0 -0.07" rpy="0 0 0" />
      <geometry>
        <cylinder radius="0.065" length="0.05"/>
      </geometry>
    </visual>
  </link>
  <gazebo reference="${namespace}/propeller_P0">
    <material>Gazebo/Grey</material>
  </gazebo>

  <joint name="${namespace}/propeller_P1_joint" type="continuous">
    <origin xyz="0.32 0.675 0" rpy="0 ${0.5*PI} 0" />
    <axis xyz="0 0 1" />
    <parent link="${namespace}/wing" />
    <child link="${namespace}/propeller_P1" />
  </joint>
  <link name="${namespace}/propeller_P1">
    <inertial>
      <mass value="0.0125" />
      <xacro:cylinder_inertia r="0.14" h="0.02" mass="0.0125" />
    </inertial>
    <visual>
      <origin xyz="0.0035 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://rotors_description/meshes/tiltwing/iris_prop_ccw.dae" scale="2.7 2.7 2.7"/>
      </geometry>
    </visual>
    <visual>
      <origin xyz="0 0 -0.07" rpy="0 0 0" />
      <geometry>
        <cylinder radius="0.065" length="0.05"/>
      </geometry>
    </visual>
  </link>
  <gazebo reference="${namespace}/propeller_P1">
    <material>Gazebo/Grey</material>
  </gazebo>

  <joint name="${namespace}/propeller_P2_joint" type="continuous">
    <origin xyz="0.32 -0.675 0" rpy="0 ${0.5*PI} 0" />
    <axis xyz="0 0 1" />
    <parent link="${namespace}/wing" />
    <child link="${namespace}/propeller_P2" />
  </joint>
  <link name="${namespace}/propeller_P2">
    <inertial>
      <mass value="0.0125" />
      <xacro:cylinder_inertia r="0.14" h="0.02" mass="0.0125" />
    </inertial>
    <visual>
      <origin xyz="0.0035 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://rotors_description/meshes/tiltwing/iris_prop_ccw.dae" scale="2.7 2.7 2.7"/>
      </geometry>
    </visual>
    <visual>
      <origin xyz="0 0 -0.07" rpy="0 0 0" />
      <geometry>
        <cylinder radius="0.065" length="0.05"/>
      </geometry>
    </visual>
  </link>
  <gazebo reference="${namespace}/propeller_P2">
    <material>Gazebo/Grey</material>
  </gazebo>

  <joint name="${namespace}/propeller_P3_joint" type="continuous">
    <origin xyz="0.32 -1.415 0" rpy="0 ${0.5*PI} 0" />
    <axis xyz="0 0 1" />
    <parent link="${namespace}/wing" />
    <child link="${namespace}/propeller_P3" />
  </joint>
  <link name="${namespace}/propeller_P3">
    <inertial>
      <mass value="0.0125" />
      <xacro:cylinder_inertia r="0.14" h="0.02" mass="0.0125" />
    </inertial>
    <visual>
      <origin xyz="0.0035 0 0" rpy="0 0 0" />
      <geometry>
        <mesh filename="package://rotors_description/meshes/tiltwing/iris_prop_ccw.dae" scale="2.7 2.7 2.7"/>
      </geometry>
    </visual>
    <visual>
      <origin xyz="0 0 -0.07" rpy="0 0 0" />
      <geometry>
        <cylinder radius="0.065" length="0.05"/>
      </geometry>
    </visual>
  </link>
  <gazebo reference="${namespace}/propeller_P3">
    <material>Gazebo/Grey</material>
  </gazebo>

  <!-- Wind -->
  <xacro:if value="${enable_wind}">
    <gazebo>
      <plugin filename="librotors_gazebo_wind_beta_plugin.so" name="wind_beta_plugin">
        <robotNamespace>${namespace}</robotNamespace>
        <windLayer>
          <layer name="layer1">
            <altitude>10</altitude>
            <windMeanNED>-5 0 0</windMeanNED>
          </layer>
          <layer name="layer2">
            <altitude>25</altitude>
            <windMeanNED>-8 2 0</windMeanNED>
          </layer>
          <layer name="layer3">
            <altitude>35</altitude>
            <windMeanNED>-10 3 0</windMeanNED>
          </layer>
        </windLayer>
        <findWind>
          <query name="q1">
            <linkName>${namespace}/base_link</linkName>
            <windTopic>wind_base_link</windTopic>
          </query>
        </findWind>
        <lambdaMin>10</lambdaMin>
        <lengthScale>725</lengthScale>
        <fourierNNN>7 7 7</fourierNNN>
        <sigma>2</sigma>
        <trnspVelTurbNED>5 0 0</trnspVelTurbNED>
      </plugin>
    </gazebo>
  </xacro:if>

  <!-- Propulsion -->
  <gazebo>
    <plugin filename="librotors_gazebo_propulsion_plugin.so" name="propulsion_plugin">
      <robotNamespace>${namespace}</robotNamespace>
      <prop name="P0">
        <linkNameRef>${namespace}/wing</linkNameRef>
        <linkNameAct>${namespace}/base_link</linkNameAct>
        <cp>0.32 1.415 0</cp>
        <axisPosRot>1 0 0</axisPosRot>
        <turnDir>ccw</turnDir>
        <speedRefTopic>P0_ref</speedRefTopic>
        <propParamsYAML>${yaml_path}/xoar_32x12.yaml</propParamsYAML>
        <tauUp>${tau_up_prop}</tauUp>
        <tauDown>${tau_down_prop}</tauDown>
        <rdpsMax>440</rdpsMax>
        <slpstrTopic>P0_slpstr</slpstrTopic>
        <visTopic>P0_vis</visTopic>
        <wind name="wind">
          <topic>wind_base_link</topic>
        </wind>
      </prop>
      <prop name="P1">
        <linkNameRef>${namespace}/wing</linkNameRef>
        <linkNameAct>${namespace}/base_link</linkNameAct>
        <cp>0.32 0.675 0</cp>
        <axisPosRot>-1 0 0</axisPosRot>
        <turnDir>cw</turnDir>
        <speedRefTopic>P1_ref</speedRefTopic>
        <propParamsYAML>${yaml_path}/xoar_32x12.yaml</propParamsYAML>
        <tauUp>${tau_up_prop}</tauUp>
        <tauDown>${tau_down_prop}</tauDown>
        <rdpsMax>440</rdpsMax>
        <slpstrTopic>P1_slpstr</slpstrTopic>
        <visTopic>P1_vis</visTopic>
        <wind name="wind">
          <topic>wind_base_link</topic>
        </wind>
      </prop>
      <prop name="P2">
        <linkNameRef>${namespace}/wing</linkNameRef>
        <linkNameAct>${namespace}/base_link</linkNameAct>
        <cp>0.32 -0.675 0</cp>
        <axisPosRot>1 0 0</axisPosRot>
        <turnDir>ccw</turnDir>
        <speedRefTopic>P2_ref</speedRefTopic>
        <propParamsYAML>${yaml_path}/xoar_32x12.yaml</propParamsYAML>
        <tauUp>${tau_up_prop}</tauUp>
        <tauDown>${tau_down_prop}</tauDown>
        <rdpsMax>440</rdpsMax>
        <slpstrTopic>P2_slpstr</slpstrTopic>
        <visTopic>P2_vis</visTopic>
        <wind name="wind">
          <topic>wind_base_link</topic>
        </wind>
      </prop>
      <prop name="P3">
        <linkNameRef>${namespace}/wing</linkNameRef>
        <linkNameAct>${namespace}/base_link</linkNameAct>
        <cp>0.32 -1.415 0</cp>
        <axisPosRot>-1 0 0</axisPosRot>
        <turnDir>cw</turnDir>
        <speedRefTopic>P3_ref</speedRefTopic>
        <propParamsYAML>${yaml_path}/xoar_32x12.yaml</propParamsYAML>
        <tauUp>${tau_up_prop}</tauUp>
        <tauDown>${tau_down_prop}</tauDown>
        <rdpsMax>440</rdpsMax>
